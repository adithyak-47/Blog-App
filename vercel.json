{
    "version": 2,
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "/server/$1"
      },
      {
        "src": "/(.*)",
        "dest": "/client/$1"
      }
    ],
    "env": {
        "MONGODB_URL": "$(cat server/.env | grep MONGODB_URL | cut -d '=' -f2)",
        "SECRET_KEY": "$(cat server/.env | grep SECRET_KEY | cut -d '=' -f2)"
      }
  }
  
  