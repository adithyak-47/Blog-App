{
    "version": 2,
    "env": {
      "MONGODB_URL": "@MONGODB_URL",
      "SECRET_KEY": "@SECRET_KEY"
    },
    "rewrites": [
      {
        "source": "/api/(.*)",
        "destination": "/server/api/$1"
      },
      {
        "source": "/(.*)",
        "destination": "/client/$1"
      }
    ]
  }
  
  
  
  